{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"acrolinx": false,
		"jQuery": false,
		"mediaWiki": true
	},
	"rules": {
		"eqeqeq": "off",
		"jsdoc/check-types": "off",
		"jsdoc/no-undefined-types": "off",
		"no-jquery/no-animate": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-sizzle": "off",
		"no-shadow": "off",
		"no-unused-vars": "off",
		"no-jquery/no-done-fail": "warn",
		"prefer-const": "warn"
	}
}
