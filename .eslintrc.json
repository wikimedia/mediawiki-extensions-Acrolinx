{
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"globals": {
		"require": false,
		"module": false,
		"mw": false,
		"OO": false,
		"acrolinx": false,
		"jQuery": false,
		"mediaWiki": true
	},
	"rules": {
		"eqeqeq": "off",
		"jsdoc/check-types": "off",
		"jsdoc/no-undefined-types": "off",
		"no-jquery/no-animate": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-sizzle": "off",
		"no-shadow": "off",
		"no-unused-vars": "off",
		"one-var": "off",
		"vars-on-top": "off"
	}
}
